{
  "name": "nebula-shield-anti-virus",
  "version": "0.1.0",
  "description": "Advanced anti-virus protection with real-time threat detection, firewall, email protection, and comprehensive security monitoring",
  "author": {
    "name": "ColinNebula",
    "email": "contact@nebulashield.com"
  },
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",

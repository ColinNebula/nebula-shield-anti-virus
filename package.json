    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "archiver": "^7.0.1",
    "axios": "^1.12.2",
    "chart.js": "^4.5.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.3",
    "electron-is-dev": "^3.0.1",
    "expo": "54.0.20",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "extract-zip": "^2.0.1",
    "framer-motion": "^11.5.4",
    "helmet": "^8.1.0",
    "idb": "^8.0.3",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^3.8.4",
    "lucide-react": "^0.441.0",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.9",
    "qrcode": "^1.5.4",
    "react": "^19.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.2.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.30.1",
    "recharts": "^2.12.7",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.1.7",
    "stripe": "^19.1.0",
    "web-vitals": "^2.1.4",
    "react-native": "0.81.5"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "check:servers": "powershell -ExecutionPolicy Bypass -File ./check-servers.ps1",
    "prebuild": "powershell -ExecutionPolicy Bypass -File ./prebuild.ps1",
    "build": "npm run check:servers && npm run prebuild && vite build",
    "preview": "vite preview",
    "start:backend": "node backend/auth-server.js",
    "start:backend:dev": "nodemon backend/auth-server.js",
    "start:all": "concurrently \"npm run start:backend:dev\" \"npm run dev\"",
    "dev:all": "concurrently \"npm run start:backend:dev\" \"npm run dev\"",
    "build:analyze": "npm run build && npx vite-bundle-visualizer",
    "test": "vitest",
    "tauri": "tauri",
    "tauri:dev": "cd src-tauri && cargo build && cd .. && tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:win": "tauri build --target x86_64-pc-windows-msvc",
    "tauri:build:debug": "tauri build --debug",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run start:backend:dev\" \"npm run dev\" \"wait-on http://localhost:3002 && electron .\"",
    "electron:build": "npm run check:servers && npm run prebuild && npm run build && electron-builder",
    "electron:build:win": "npm run check:servers && npm run prebuild && npm run build && electron-builder --win",
    "electron:build:standalone": "powershell -ExecutionPolicy Bypass -File ./build-standalone.ps1",
    "build:standalone": "powershell -ExecutionPolicy Bypass -File ./build-standalone.ps1",
    "electron:build:installer": "powershell -ExecutionPolicy Bypass -File ./build-installer.ps1",
    "build:installer": "powershell -ExecutionPolicy Bypass -File ./build-installer.ps1",
    "electron:build:win:fast": "powershell -ExecutionPolicy Bypass -File ./build-optimized.ps1",
    "electron:build:win:portable": "cross-env NODE_ENV=production npm run build && electron-builder --win --dir",
    "electron:build:win:portable:exe": "cross-env NODE_ENV=production npm run build && electron-builder --win portable",
    "electron:build:win:production": "powershell -ExecutionPolicy Bypass -File ./build-production.ps1",
    "electron:build:mac": "npm run check:servers && npm run prebuild && npm run build && electron-builder --mac",
    "electron:build:linux": "npm run check:servers && npm run prebuild && npm run build && electron-builder --linux",
    "pack": "electron-builder --dir",
    "dist": "npm run check:servers && npm run build && electron-builder",
    "dist:win": "npm run check:servers && npm run build && electron-builder --win --x64",
    "dist:mac": "npm run check:servers && npm run build && electron-builder --mac",
    "dist:linux": "npm run check:servers && npm run build && electron-builder --linux",
    "security:audit": "npm audit --audit-level=moderate",
    "security:audit:production": "npm audit --production --audit-level=moderate",
    "security:audit:fix": "npm audit fix",
    "security:audit:full": "npm audit --json > security-audit.json",
    "security:check": "powershell -ExecutionPolicy Bypass -File ./security-audit.ps1",
    "security:scan": "npm run security:audit:production && npm run verify-integrity",
    "security:pre-commit": "npm run security:check",
    "security:all": "npm run security:check && npm run security:audit:production",
    "integrity:generate": "node src/utils/integrityChecker.js generate",
    "integrity:verify": "node src/utils/integrityChecker.js verify",
    "integrity:watch": "node src/utils/integrityChecker.js watch",
    "integrity:report": "node src/utils/integrityChecker.js report",
    "verify-integrity": "node src/utils/integrityChecker.js verify",
    "generate-checksums": "node src/utils/integrityChecker.js generate",
    "watch-files": "node src/utils/integrityChecker.js watch",
    "integrity-report": "node src/utils/integrityChecker.js report",
    "security-check": "npm run verify-integrity && npm run security:audit:production",
    "pre-commit-security": "npm run security:check",
    "generate-icons": "node build-resources/generate-all-icons.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "deploy": "railway up",
    "deploy:backend": "cd backend && railway up",
    "deploy:railway": "powershell -ExecutionPolicy Bypass -File ./deploy-to-railway.ps1 -Deploy"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.1.0",
    "@types/react": "~19.1.10",
    "@vitejs/plugin-react": "5.1.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^38.3.0",
    "electron-builder": "^26.0.12",
    "electron-icon-maker": "^0.0.5",
    "electron-reload": "^2.0.0-alpha.1",
    "http-proxy-middleware": "^3.0.5",
    "nodemon": "^3.1.10",
    "png-to-ico": "^3.0.1",
    "sharp": "^0.34.4",
    "source-map-explorer": "^2.5.3",
    "svg2png-cli": "^1.1.1",
    "terser": "5.44.0",
    "to-ico": "1.0.1",
    "vite": "7.1.12",
    "wait-on": "^9.0.1",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
